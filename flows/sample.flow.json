{
  "key": "sample-workflow",
  "tags": ["example", "demo"],
  "attributes": {
    "type": "F",
    "labels": [
      {
        "label": "Sample Workflow",
        "language": "en"
      }
    ],
    "timeout": {
      "key": "workflow-timeout",
      "target": "final-state",
      "versionStrategy": "Minor",
      "timer": {
        "reset": "N",
        "duration": "PT5M"
      }
    },
    "startTransition": {
      "key": "start",
      "target": "initial-state",
      "triggerType": 0,
      "versionStrategy": "Minor",
      "labels": [
        {
          "label": "Start Workflow",
          "language": "en"
        }
      ]
    },
    "states": [
      {
        "key": "initial-state",
        "stateType": 1,
        "versionStrategy": "Minor",
        "labels": [
          {
            "label": "Initial State",
            "language": "en"
          }
        ],
        "transitions": [
          {
            "key": "to-intermediate",
            "from": "initial-state",
            "target": "intermediate-state",
            "triggerType": 1,
            "versionStrategy": "Minor",
            "labels": [
              {
                "label": "To Intermediate",
                "language": "en"
              }
            ]
          }
        ]
      },
      {
        "key": "intermediate-state",
        "stateType": 2,
        "versionStrategy": "Minor",
        "labels": [
          {
            "label": "Intermediate State",
            "language": "en"
          }
        ],
        "transitions": [
          {
            "key": "to-final",
            "from": "intermediate-state",
            "target": "final-state",
            "triggerType": 1,
            "versionStrategy": "Minor",
            "labels": [
              {
                "label": "To Final",
                "language": "en"
              }
            ]
          }
        ]
      },
      {
        "key": "final-state",
        "stateType": 3,
        "stateSubType": 1,
        "versionStrategy": "Minor",
        "labels": [
          {
            "label": "Final State",
            "language": "en"
          }
        ]
      }
    ]
  }
}